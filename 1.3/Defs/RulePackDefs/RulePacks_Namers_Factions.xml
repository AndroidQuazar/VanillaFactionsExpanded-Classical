<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RulePackDef>
    <defName>VFEC_NamerSettlementClassical</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[ClassicalName]</li>
      </rulesStrings>
      <rulesFiles>
        <li>ClassicalName->Names/Classical_Towns</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VFEC_NamerPersonClassicalUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>Classical_First_Male->Names/Classical_First_Male</li>
        <li>Classical_First_Female->Names/Classical_First_Female</li>
		<li>Classical_Last->Names/Classical_Last</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VFEC_NamerPersonClassical</defName>
	<include>
      <li>VFEC_NamerPersonClassicalUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Classical_First_Male] [Classical_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>VFEC_NamerPersonClassicalFemale</defName>
	<include>
      <li>VFEC_NamerPersonClassicalUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Classical_First_Female] [Classical_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VFEC_LeaderTitleMaker_Classical</defName>
    <rulePack>
      <rulesStrings>
        <li>r_leaderTitle->Senator</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <!-- republics --> 
  <RulePackDef>
    <defName>VFEC_NamerFactionCentral</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->Central Republic</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VFEC_NamerFactionWestern</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->Western Republic</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VFEC_NamerFactionEastern</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->Eastern Republic</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>