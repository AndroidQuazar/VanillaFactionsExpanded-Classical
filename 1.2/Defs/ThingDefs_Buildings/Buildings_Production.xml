<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BenchBase">
		<defName>VFEC_MeatDryingRack</defName>
		<label>meat drying rack</label>
		<description>Description needed</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<researchPrerequisites>
			<li>VFEC_Research_MeatDrying</li>
		</researchPrerequisites>
		<graphicData>
			<texPath>Things/Building/Production/MeatDryingRack/MeatDryingRack</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4,4)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<costStuffCount>75</costStuffCount>
		<costList>
			<WoodLog>20</WoodLog>
		</costList>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
		</stuffCategories>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>2000</WorkToBuild>
			<MaxHitPoints>180</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Cleanliness>-15</Cleanliness>
		</statBases>
		<size>(2,2)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<comps>
			<!--Input selection button is still showing-->
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Buildings/Production/MeatDryingRack/MeatDryingRackFull</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<qualitySelector>false</qualitySelector>
				<acceptsNoneAsInput>false</acceptsNoneAsInput>
				<InsertFirstItemDesc>VCE_InsertMilkDesc</InsertFirstItemDesc>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<isCategoryBuilding>true</isCategoryBuilding>
				<resetSemiautomaticIcon>UI/Buttons/MeatDryingRack/VCE_ResetMilk</resetSemiautomaticIcon>
				<chooseIngredientsIcon>UI/Buttons/MeatDryingRack/VCE_EmptyMilkIcon</chooseIngredientsIcon>
				<bringIngredientsText>VCE_StartInsertionMilk</bringIngredientsText>
				<bringIngredientsDesc>VCE_StartInsertionMilkDesc</bringIngredientsDesc>
				<bringIngredientsIcon>UI/Buttons/MeatDryingRack/VCE_InsertMilk</bringIngredientsIcon>
				<cancelIngredientsText>VCE_CancelBringingMilk</cancelIngredientsText>
				<cancelIngredientsDesc>VCE_CancelBringingMilkDesc</cancelIngredientsDesc>
				<cancelIngredientsIcon>UI/Buttons/MeatDryingRack/VCE_CancelMilk</cancelIngredientsIcon>
				<noPowerDestroysProgress>false</noPowerDestroysProgress>
				<removeProductText>VCE_RemoveCheese</removeProductText>
				<removeProductDesc>VCE_RemoveCheeseDesc</removeProductDesc>
				<removeProductIcon>UI/Buttons/MeatDryingRack/VCE_RemoveCheese</removeProductIcon>
				<destroyIngredientsAtStartOfProcess>false</destroyIngredientsAtStartOfProcess>
				<destroyIngredientsAtAwfulQuality>true</destroyIngredientsAtAwfulQuality>
				<isRainDependingMachine>true</isRainDependingMachine>
				<isTemperatureDependingMachine>true</isTemperatureDependingMachine>
				<minTemp>10</minTemp>
				<maxTemp>100</maxTemp>
				<isLightDependingMachine>true</isLightDependingMachine>
				<minLight>0</minLight>
				<maxLight>1</maxLight>
				<isMachineSpecifiesOutput>true</isMachineSpecifiesOutput>
			</li>
		</comps>
		<designationHotKey>Misc3</designationHotKey>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
	</ThingDef>
